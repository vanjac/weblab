{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"moduleDetection": "force",
		"allowJs": true,
		"checkJs": true,
		"skipLibCheck": false,
		"noEmit": true,
		"composite": true,
		"outDir": "build",

		"strict": true,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"noImplicitReturns": true
	}
}
