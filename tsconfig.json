{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"moduleDetection": "force",
		"allowJs": true,
		"checkJs": true,
		"skipLibCheck": false,
		"noEmit": true,
		"pretty": false,

		"strict": true,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"noImplicitReturns": true
	},
	"exclude": [
		"./lib"
	]
}
